<script>

    function gd(date) {
        const d = date.split('/');
        const year = Number.parseInt('20'.concat(d[2]));
        const month = Number.parseInt(d[0]) - 1;
        const day = Number.parseInt(d[1]) - 1
        const nd = new Date(year, month, day).getTime();
        return nd
    }

    const settings_url = '{% if url_prefix %}/{{ url_prefix}}{% endif %}/settings.html';

    const url_prefix = '{% if url_prefix %}/{{ url_prefix}}{% endif %}';

    const chart_max = {{ totals.confirmed }};

    const tickArray = [{% for date in dates %}gd("{{ date }}"){% if loop.index != dates|length %}, {% endif %}{% endfor %}];

    const earliestDate = gd("{{ dates[0] }}");

    const latestDate = gd("{{ dates[-1] }}");

    const confirmed_time_series = [{% for data in chart_data.confirmed %}[gd("{{ dates[loop.index0] }}"), {{ data[1] }}]{% if loop.index != dates|length %}, {% endif %}{% endfor %}];
    const recovered_time_series = [{% for data in chart_data.recovered %}[gd("{{ dates[loop.index0] }}"), {{ data[1] }}]{% if loop.index != dates|length %}, {% endif %}{% endfor %}];
