    const delta_recovered_time_series = [{% for data in delta_chart_data.recovered %}[gd("{{ dates[loop.index0] }}") - 5000000 * 4, {{ data[1] }}]{% if loop.index != dates|length %}, {% endif %}{% endfor %}];
    const delta_deaths_time_series = [{% for data in delta_chart_data.deaths %}[gd("{{ dates[loop.index0] }}") - 5000000, {{ data[1] }}]{% if loop.index != dates|length %}, {% endif %}{% endfor %}];

    const confirmed_trend_series = [{% for data in trend_chart_data.confirmed %}[gd("{{ dates[loop.index0] }}"), {{ data[1] }}]{% if loop.index != dates|length %}, {% endif %}{% endfor %}];

    var previousPoint = null;

    const region_map_data = {{ region_map_data|tojson }};
</script>

