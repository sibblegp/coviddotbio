        function plotMap() {
            var max = 0,
                min = 0,
                cc,
                startColor = [200, 238, 255],
                endColor = [0, 100, 145],
                colors = {},
                hex;

            //find maximum and minimum values
            for (cc in region_map_data) {
                if (parseInt(region_map_data[cc]) > max) {
                    max = parseInt(region_map_data[cc]);
                }
                if (parseInt(region_map_data[cc]) < min) {
                    min = parseInt(region_map_data[cc]);
                }
            }

            //set colors according to values of GDP
            for (cc in region_map_data) {
                if (region_map_data[cc] > 0) {
                    colors[cc] = '#';
                    for (var i = 0; i < 3; i++) {
                        hex = Math.round(startColor[i]
                            + (endColor[i]
                                - startColor[i])
                            * (region_map_data[cc] / (max - min))).toString(16);

                        if (hex.length == 1) {
                            hex = '0' + hex;
                        }

                        colors[cc] += (hex.length == 1 ? '0' : '') + hex;
